# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Robot Audio Device Configuration
# Based on audio_report.txt analysis:
# - Device 0: USB Audio Device (robot's speakers) - RECOMMENDED
# - Device 38: default (fallback)
# - Device 33: pulse (fallback)
ROBOT_AUDIO_OUTPUT_DEVICE=0

# - Device 25: pulse (robot's microphone) - RECOMMENDED  
# - Device 27: default (fallback)
# - Device 0: USB mic (has sample rate issues)
ROBOT_AUDIO_INPUT_DEVICE=25

# Audio Sample Rates
ROBOT_AUDIO_SAMPLE_RATE=24000
